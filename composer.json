{
    "name": "dmitryrogolev/is",
    "description": "Functional roles for the Laravel framework.",
    "version": "0.0.3",
    "type": "library",
    "keywords": [
        "roles",
        "level",
        "is",
        "laravel",
        "laravel-roles",
        "laravel-level",
        "laravel-is"
    ],
    "homepage": "https://github.com/dmitry-rogolev/Is",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Rogolev",
            "email": "work.drogolev@internet.ru",
            "homepage": "https://dmitryrogolev.ru",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:dmitry-rogolev/Slug.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dmitry-rogolev/Service.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dmitry-rogolev/Helper.git"
        }
    ],
    "require": {
        "laravel/framework": "^10.30",
        "dmitryrogolev/service": "^0.0.1",
        "dmitryrogolev/slug": "^0.0.1",
        "dmitryrogolev/helper": "^0.0.3"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^8.21",
        "nunomaduro/collision": "^7.10"
    },
    "autoload": {
        "psr-4": {
            "dmitryrogolev\\Is\\": "src/",
            "dmitryrogolev\\Is\\Database\\Factories\\": "database/factories/",
            "dmitryrogolev\\Is\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dmitryrogolev\\Is\\Tests\\": "tests/",
            "dmitryrogolev\\Is\\Workbench\\App\\": "workbench/app/",
            "dmitryrogolev\\Is\\Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "dmitryrogolev\\Is\\Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "dmitryrogolev\\Is\\Providers\\IsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "@php vendor/bin/testbench package:test",
        "pint": "@php vendor/bin/pint",
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "@php vendor/bin/pint",
            "@php vendor/bin/phpstan analyse"
        ]
    }
}
