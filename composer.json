{
    "name": "dmitryrogolev/is",
    "description": "Functional roles for the Laravel framework.",
    "version": "0.0.1",
    "type": "library",
    "keywords": [
        "roles",
        "level",
        "is",
        "laravel",
        "laravel-roles",
        "laravel-level",
        "laravel-is"
    ],
    "homepage": "https://github.com/dmitry-rogolev/Is",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Rogolev",
            "email": "work.drogolev@internet.ru",
            "homepage": "https://dmitryrogolev.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^10.30"
    },
    "require-dev": {
        "orchestra/testbench": "^8.14"
    },
    "autoload": {
        "psr-4": {
            "dmitryrogolev\\Is\\": "src/",
            "dmitryrogolev\\Is\\Database\\": "database/", 
            "dmitryrogolev\\Is\\Database\\Migrations\\": "database/migrations/", 
            "dmitryrogolev\\Is\\Database\\Factories\\": "database/factories/", 
            "dmitryrogolev\\Is\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dmitryrogolev\\Is\\Tests\\": "tests/", 
            "dmitryrogolev\\Is\\Tests\\Seeders\\": "tests/seeders/", 
            "dmitryrogolev\\Is\\Tests\\Migrations\\": "tests/migrations/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "dmitryrogolev\\Is\\Providers\\IsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
